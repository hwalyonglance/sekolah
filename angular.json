{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"sekolah": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"prefix": "",
			"schematics": {
				"@schematics/angular:component": {
					"flat": true,
					"inlineTemplate": true,
					"inlineStyle": true,
					"skipImport":true,
					"spec": false,
					"styleext": "scss"
				},
				"@schematics/angular:guard": {
					"spec": false
				},
				"@schematics/angular:directive": {
					"spec": false
				},
				"@schematics/angular:service": {
					"spec": false
				}
			},
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/sekolah",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.app.json",
						"serviceWorker": false,
						"assets": [
							"src/favicon.ico",
							"src/assets"
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"hmr": {
							"fileReplacements": [
								{
									"replace": "./src/environments/environment.ts",
									"with": "./src/environments/environment.hmr.ts"
								}
							]
						},
						"production": {
							"aot":true,
							"baseHref":"http://localhost/sekolah/",
							"deployUrl":"http://localhost/sekolah/assets/",
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								},
								{
									"replace": "src/polyfills.ts",
									"with": "src/polyfills.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "none",
							"serviceWorker": false,
							"sourceMap": false
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "sekolah:build"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"src/tsconfig.app.json",
							"src/tsconfig.spec.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "sekolah:build"
					},
					"configurations": {
						"hmr": {
							"hmr": true,
							"browserTarget": "sekolah:build:hmr"
						},
						"production": {
							"browserTarget": "sekolah:build:production"
						}
					}
				},
				"server": {
					"builder": "@angular-devkit/build-angular:server",
					"options": {
						"outputPath": "dist/sekolah-server",
						"main": "src/main.server.ts",
						"tsConfig": "src/tsconfig.server.json"
					}
				}
			}
		}
	},
	"defaultProject": "sekolah"
}
